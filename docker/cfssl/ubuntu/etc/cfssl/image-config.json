{
    "update": false,
    "config": "/etc/cfssl/cfssl-simple.json",
    "db": "/etc/cfssl/db.json",
    "environment": "production",
    "csr": {
        "ca": [{
            "default": true,
            "name": "ca", 
            "path": "/etc/cfssl/root-ca-csr.json"}
